	<variable name="totalFemale" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{female}]]></variableExpression>
	</variable>
	<variable name="totalOther" class="java.math.BigDecimal" calculation="Sum">
