	<queryString>
		<![CDATA[select organization.name as organization_name, sample.accession_number, sample.received_date as date, type_of_sample.description as sample, patient_identity.identity_data as registration_number, test.description as test,
result.result_type,
result.id as result_id,
result.value,
(case organization.name WHEN NULL THEN ''
		ELSE (case result.result_type WHEN NULL THEN ''
					WHEN 'N' then result.value
					WHEN 'R' then result.value
	 				WHEN 'D' then dictionary.dict_entry
	 				ELSE 'N/A'
		END)
END) as result
from referral
inner join organization on referral.organization_id = organization.id
inner join analysis on referral.analysis_id = analysis.id
inner join test on analysis.test_id = test.id
inner join result on result.analysis_id=analysis.id
inner join sample_item on sample_item.id = analysis.sampitem_id
inner join sample on sample.id = sample_item.samp_id
inner join sample_human on sample_human.samp_id = sample.id
inner join patient_identity on patient_identity.patient_id = sample_human.patient_id
inner join type_of_sample on sample_item.typeosamp_id = type_of_sample.id
left  join dictionary on result.result_type='D' and dictionary.id::varchar(255) = result.value
where ((result.result_type='N' AND result.value != '' AND result.value IS NOT NULL AND (cast(result.value as double precision) NOT BETWEEN result.min_normal AND result.max_normal)) OR (result.result_type='D' and dictionary.local_abbrev = 'POSITIVE'))
AND sample.entered_date >= $P{from_date} AND sample.entered_date <= $P{to_date} and referral.canceled = FALSE 
order by organization_name asc, sample.received_date desc, accession_number asc;;]]>
	</queryString>
